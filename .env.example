# AWS Configuration
AWS_REGION=eu-west-1
AWS_ACCOUNT_ID=123456789012
CDK_DEFAULT_ACCOUNT=123456789012
CDK_DEFAULT_REGION=eu-west-1

# Environment
ENVIRONMENT=dev

# OAuth Configuration (to be obtained from Google and Microsoft)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret

# Frontend Configuration
NEXT_PUBLIC_API_URL=https://your-api-gateway-url
NEXT_PUBLIC_AWS_REGION=eu-west-1
NEXT_PUBLIC_COGNITO_USER_POOL_ID=eu-west-1_xxxxxxxxx
NEXT_PUBLIC_COGNITO_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
NEXT_PUBLIC_MICROSOFT_CLIENT_ID=your-microsoft-client-id

# Bedrock Configuration
BEDROCK_MODEL_ID=eu.amazon.nova-pro-v1:0
BEDROCK_REGION=eu-west-1

# KMS Configuration (will be set by CDK deployment)
KMS_KEY_ID=alias/meeting-scheduling-agent-dev